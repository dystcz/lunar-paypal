{
    "name": "dystcz/lunar-paypal",
    "description": "Lunar PayPal integration for Laravel",
    "keywords": [
        "dystcz",
        "lunar-paypal",
        "lunar",
        "laravel",
        "php"
    ],
    "homepage": "https://github.com/dystcz/lunar-paypal",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jakub Theimer",
            "email": "jakub@dy.st",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "laravel/pint": "^1.7",
        "driftingly/rector-laravel": "^0.17.0",
        "nunomaduro/larastan": "^2.5.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "rector/rector": "^0.15.23",
        "spatie/laravel-ray": "^1.32"
    },
    "autoload": {
        "psr-4": {
            "Dystcz\\LunarPaypal\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dystcz\\LunarPaypal\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage-html coverage",
        "analyse": "vendor/bin/phpstan analyse"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dystcz\\LunarPaypal\\LunarPaypalServiceProvider"
            ],
            "aliases": {
                "LunarPaypal": "Dystcz\\LunarPaypal\\LunarPaypalFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
